
@using SXF.Utils;

@{
    ViewBag.Title = "CompanyInfoSet";
}
@{
    Layout = "_UserAdmin.cshtml";
}
@section pageStyle{


}

<div class="list">
    <div class="list-item padding">
        @using (Html.BeginForm(ViewContext.RouteData.Values["action"].ToString(), ViewContext.RouteData.Values["Controller"].ToString(), FormMethod.Post, new { @class = "form-inline", id = "search_form", role = "form", style = "" }))
        {

            @Html.AntiForgeryToken()
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })

            <div>
                <div class="form-group col-md-12 margin-bottom-10">
                    <label class="col-sm-2 control-label">客服QQ</label>
                    <div class="col-sm-10">
                        <input type="text" id="QQ" name="QQ" value="@ViewBag.QQ" />
                    </div>
                </div>
                <div class="form-group col-md-12 margin-bottom-10">
                    <label class="col-sm-2 control-label">客服微信</label>
                    <div class="col-sm-10">
                        <input type="text" id="WeChat" name="WeChat" value="@ViewBag.WeChat" />
                    </div>
                </div>
                <div class="form-group col-md-12 margin-bottom-10">
                    <label class="col-sm-2 control-label">加盟电话</label>
                    <div class="col-sm-10">
                        <input type="text" id="Phone" name="Phone" value="@ViewBag.Phone" />
                    </div>
                </div>
                <div class="form-group col-md-12 margin-bottom-10">
                    <input type="submit" value="保存" class="btn btn-primary btn-sm" id="ueForm" />
                </div>
            </div>
        }
    </div>

</div>

@section scripts{
    <script src="~/Scripts/app/appBase.js"></script>
    <script type="text/javascript">
        $(function () {

        });
    </script>

}
<script type="text/html" id="tmpl_blank">
</script>



